alias h='history'
alias hg='history 0 | grep'

alias la='ls -AF'
alias ll='ls -AlF'
alias lh='ls -d .*'
alias llh='ls -dl .*'

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias ......='cd ../../../../..'
alias cd..='cd ..'
alias cd...='cd ../..'
alias cd....='cd ../../..'
alias cd.....='cd ../../../..'
alias cd......='cd ../../../../..'
